{
  "name": "@thor-commerce/backendFlashCards",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "node-env=development"
            ]
          }
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@thor-commerce/backendFlashCards:build",
          "runBuildTargetDependencies": false,
          "inspect": true,
          "host": "localhost"
        },
        "configurations": {
          "development": {
            "buildTarget": "@thor-commerce/backendFlashCards:build:development"
          },
          "production": {
            "buildTarget": "@thor-commerce/backendFlashCards:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  },
  "dependencies": {
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@nestjs/sequelize": "^11.0.0",
    "@types/sequelize": "^4.28.20",
    "run-script-webpack-plugin": "^0.2.3",
    "sequelize-cli": "^6.6.3",
    "webpack-node-externals": "^3.0.0"
  }
}
